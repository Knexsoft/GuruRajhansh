
@{
    ViewData["Title"] = "Index";
}

<section class="globle-info" ng-controller="ctrDashboard">
    <div class="panel">
        <div class="panel-heading">
            <h3><i class="fa fa-dashboard"></i>&nbsp;Dashboard</h3>
        </div>
        <div class="panel-body mt-5" ng-init="GetTeamInfo()">
            <div class="row">
                <div class="col-lg-3">
                    <div class="small-box bg-success">
                        <div class="inner">
                            <h3><span>&nbsp;</span>{{ActiveUsers}}</h3>
                            <p>Active Users</p>
                        </div>
                        <div class="icon">
                            <i class="fa fa-user"></i>
                        </div>
                    </div>
                </div>
                <div class="col-lg-3">
                    <div class="small-box bg-danger">
                        <div class="inner">
                            <h3><span>&nbsp;</span>{{UnactiveUsers}}</h3>
                            <p>Unactive Users</p>
                        </div>
                        <div class="icon">
                            <i class="fa fa-user-times"></i>
                        </div>
                    </div>
                </div>
                <div class="col-lg-3">
                    <div class="small-box bg-primary">
                        <div class="inner">
                            <h3><i class="fa fa-rupee"></i>{{TotalIncome}}</h3>
                            <p>Total Income</p>
                        </div>
                        <div class="icon">
                            <i class="fa fa-rupee"></i>
                        </div>
                    </div>
                </div>
                <div class="col-lg-3">
                    <div class="small-box bg-warning">
                        <div class="inner">
                            <h3><i class="fa fa-rupee"></i>2800</h3>
                            <p>Total withdrawal</p>
                        </div>
                        <div class="icon">
                            <i class="fa fa-angle-double-down"></i>
                        </div>
                    </div>
                </div>

            </div>

        </div>

    </div>
</section>
<section class="User-info table-light p-3">
    <div class="panel">
        <div class="panel-heading">
            <h4><i class="fa fa-users"></i>&nbsp;Users</h4>
        </div>
        <div class="panel-body">
            <div class="row">
                <div class="col-lg-12">
                    <user-table></user-table>
                </div>
            </div>
        </div>
    </div>
</section>

