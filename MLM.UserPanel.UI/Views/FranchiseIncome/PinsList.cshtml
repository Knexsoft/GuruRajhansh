
@{
    ViewData["Title"] = "PinsList";
}

<h2>PinsList</h2>
<section class="User-info" ng-controller="ctrFranchise">
    <div class="panel">
        <div class="panel-heading">
            <h4><i class="fa fa-key"></i>&nbsp;Pin List</h4>
        </div>
        <div class="panel-body">
            <div class="row">
                <div class="col-lg-12">
                    <table class="table table-bordered white-bg" ng-init="getPinListByFrenchiseID()">
                        <thead>
                            <tr>
                                <th>Sr No</th>
                                <th>Total Pins</th>
                                <th>Is Used Pin</th>
                                <th>Buy Date</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr ng-repeat="fit in franchises">
                                <td>{{$index+1}}</td>
                                <td>{{fit.pin}}</td>
                                <td>{{fit.isUsed === false ? 'Active Key' : 'Key Used'}}</td>
                                <td>{{fit.createdOn | date:'medium'}}</td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</section>

